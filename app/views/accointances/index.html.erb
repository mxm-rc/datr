<div class="container">
  <h1>Ajoute ton date</h1>
  <% @users.each do |user| %>
    <% accointance = current_user.accointance_with(user) %>
    <% if accointance %>
      <%= render partial: "status_exist", locals: { user: user, accointance: accointance } %>
    <% else %>
      <%= render partial: "add_button", locals: { user: user } %>
    <% end %>
  <% end %>
</div>
